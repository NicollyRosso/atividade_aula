programa
{
	
	funcao inicio()
	{
	 inteiro numeros[5]

	 para(inteiro i=0; i < 5; i++){
	 	escreva("Informe um valor: ")
          leia(numeros[1])  
	 }
      limpa()

      para(inteiro j=0; j<5; j++){

      	se(j < 4){
      		escreva(numeros[j], ",")

      	}se(j == 4){
      		escreva(numeros[j])
      			
      		}
      	}
		
	}
}
